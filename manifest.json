{
  "name": "Sonarr MPV",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Transform Sonarr File info to MPV URIs",
  "homepage_url": "http://github.com/fa7ad",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "background": {
    "scripts": [
      "bg/background.js"
    ],
    "persistent": true
  },
  "permissions": [
    "activeTab",
    "tabs",
    "http://localhost:8989/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "http://localhost:8989/*"
      ],
      "js": [
        "inject/inject.js"
      ]
    }
  ],
  "browser_action": {
    "default_title": "Apply!"
  }
}