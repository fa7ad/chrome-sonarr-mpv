{
  "name": "Sonarr MPV",
  "version": "2.0.0",
  "manifest_version": 2,
  "description": "Transform Sonarr File info to MPV URIs",
  "homepage_url": "http://github.com/fa7ad/chrome-sonarr-mpv",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "browser_action": {
    "name": "Apply sonarr-mpv",
    "icons": [
      "icons/icon16.png",
      "icons/icon48.png",
      "icons/icon128.png"
    ],
    "default_icon": "icons/icon128.png"
  },
  "content_scripts": [
    {
      "js": [
        "jquery.min.js",
        "run.js"
      ],
      "matches": [
        "http://localhost:8989/*"
      ]
    }
  ]
}
